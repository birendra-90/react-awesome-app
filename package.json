{
  "name": "react-awesome-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon ./bin/www",
    "server:start": "pm2 start ./bin/www --name yeps --watch",
    "server:stop": "pm2 kill",
    "db:start": "docker run -d --name redis -p 6379:6379 redis:alpine",
    "db:stop": "docker rm -f redis",
    "docker:build": "docker build -t yeps .",
    "docker: run": "docker run -d -p 80:3000 --name yeps -v $PWD:/usr/src/app --link redis:redis yeps",
    "docker:stop": "docker rm -f yeps",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down --rmi all",
    "lint": "npm-run-all lint:**",
    "lint:js": "eslint server tests",
    "test": "npm-run-all test:**",
    "test:lint": "npm run lint",
    "test:server": "mocha tests --recursive",
    "test:security": "nsp check",
    "test:coverage": "istanbul cover _mocha -- tests --recursive"
  },
  "keywords": [],
  "author": "Evheniy Bystrov",
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "pm2": "^2.7.2",
    "yeps": "^1.0.1",
    "yeps-bodyparser": "^1.1.1",
    "yeps-cors": "^1.0.1",
    "yeps-error": "^1.2.2",
    "yeps-helmet": "^1.0.1",
    "yeps-logger": "^1.0.1",
    "yeps-mongoose": "^1.0.1",
    "yeps-redis": "^1.0.1",
    "yeps-router": "^1.2.0",
    "yeps-server": "^1.1.0",
    "yeps-static": "^1.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "sinon": "^4.1.2"
  }
}
